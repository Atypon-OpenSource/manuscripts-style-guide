{
  "name": "@manuscripts/style-guide",
  "description": "Shared components for Manuscripts applications",
  "version": "0.0.1",
  "repository": "https://gitlab.com/mpapp-private/manuscripts-style-guide",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "keywords": [
    "react"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "build-storybook": "build-storybook",
    "dev": "tsc --outDir dist/es --watch",
    "lint": "tslint --project .",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"{src,stories}/**/*.{ts,tsx}\"",
    "preversion": "npm-run-all --parallel lint typecheck test",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "publishConfig": {
    "@manuscripts:registry": "https://atypon-npm-registry.herokuapp.com/"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@manuscripts/assets": "^0.3.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/jest": "^23.3.7",
    "@types/jsdom": "^12.2.0",
    "@types/lodash-es": "^4.17.1",
    "@types/react": "^16.6.3",
    "@types/react-dom": "^16.6.3",
    "@types/react-modal": "^3.8.0",
    "@types/react-router-dom": "^4.2.3",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "formik": "^1.3.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.10.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "styled-components": "^4.1.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.4.0",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "@manuscripts/assets": "^0.3.0",
    "formik": "^1.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.2.2",
    "styled-components": "^4.1.3"
  },
  "dependencies": {
    "lodash-es": "^4.17.11",
    "react-modal": "^3.8.1"
  }
}
