    "prosemirror-commands": "1.5.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-modal": "^3.16.1",
    "react-modern-calendar-datepicker": "^3.1.6",
    "react-router-dom": "^5.3.4",
    "react-select": "^5.7.3",
    "react-sizeme": "^3.0.2",
    "react-tooltip": "^5.26.0",
    "styled-components": "^5.2.0"
