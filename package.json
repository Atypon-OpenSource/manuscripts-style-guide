{
  "name": "@manuscripts/style-guide",
  "description": "Shared components for Manuscripts applications",
  "version": "0.13.2",
  "repository": "gitlab:mpapp-public/manuscripts-style-guide",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist",
    "styles"
  ],
  "keywords": [
    "react"
  ],
  "scripts": {
    "build": "npm-run-all --parallel 'build:* -- --project tsconfig.build.json'",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "build-storybook": "build-storybook",
    "dev": "npm-run-all --parallel 'build:* --project tsconfig.build.json --watch'",
    "lint": "tslint --project .",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"{src,stories}/**/*.{ts,tsx}\"",
    "preversion": "npm-run-all --parallel lint typecheck test",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@manuscripts/manuscripts-json-schema": "^1.17.2",
    "@manuscripts/publish": "^0.2.2",
    "@manuscripts/tslint-config": "^0.1.4",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/jest": "^23.3.7",
    "@types/jsdom": "^12.2.0",
    "@types/lodash-es": "^4.17.1",
    "@types/react": "^16.6.3",
    "@types/react-dom": "^16.6.3",
    "@types/react-modal": "^3.8.0",
    "@types/react-select": "^2.0.13",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "formik": "^1.3.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.10.2",
    "react": "^16.6.3",
    "react-dnd-html5-backend": "^9.4.0",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2",
    "styled-components": "^4.1.3",
    "tslint": "^5.9.1",
    "typeface-lato": "^0.0.75",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "formik": "^1.3.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "dependencies": {
    "@manuscripts/assets": "^0.5.11",
    "lodash-es": "^4.17.11",
    "react-dnd": "^9.4.0",
    "react-modal": "^3.8.1",
    "react-select": "^2.4.1",
    "react-sizeme": "^2.6.7",
    "tooltip.js": "^1.3.2"
  }
}
