{
  "name": "@manuscripts/style-guide",
  "description": "Shared components for Manuscripts applications",
  "version": "3.1.0",
  "repository": "github:Atypon-OpenSource/manuscripts-style-guide",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist",
    "styles"
  ],
  "keywords": [
    "react"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "build-storybook": "storybook build",
    "dev": "npm-run-all --parallel 'build:* --watch'",
    "lint": "eslint src stories --ext .ts,.tsx --max-warnings=0",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"{src,stories}/**/*.{ts,tsx}\"",
    "preversion": "npm-run-all --parallel lint typecheck test",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "pnpm build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "2.2.3",
    "@manuscripts/json-schema": "2.2.12",
    "date-fns": "2.30.0",
    "formik": "2.4.6",
    "lodash": "4.17.21",
    "prosemirror-commands": "1.7.1",
    "prosemirror-history": "1.4.1",
    "prosemirror-keymap": "1.2.3",
    "prosemirror-model": "1.25.0",
    "prosemirror-state": "1.4.3",
    "prosemirror-view": "1.40.0",
    "react": "18.3.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "react-modal": "3.16.3",
    "react-modern-calendar-datepicker": "3.1.6",
    "react-router-dom": "5.3.4",
    "react-select": "5.10.1",
    "react-sizeme": "3.0.2",
    "react-tooltip": "5.28.1",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.29.0",
    "@manuscripts/eslint-config": "0.5.1",
    "@storybook/addon-actions": "7.6.10",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-interactions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/blocks": "7.6.10",
    "@storybook/react": "7.6.10",
    "@storybook/react-webpack5": "7.6.10",
    "@storybook/test": "7.6.10",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/react": "18.3.21",
    "@types/react-dom": "18.3.7",
    "@types/react-modal": "3.16.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "babel-jest": "29.7.0",
    "eslint": "9.29.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-mdx": "3.5.0",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.0",
    "rimraf": "6.0.1",
    "storybook": "7.6.10",
    "typeface-lato": "1.1.13",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}