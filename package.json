    "@storybook/react": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.202",
    "@types/styled-components": "^5.1.4",
    "@types/react": "^17.0.2",
